   "source": [
    "import zipfile\n",
    "def unzip(filename):\n",
    "    with zipfile.ZipFile(filename,'r') as zf:\n",
    "        inner_zip=zf.namelist()\n",
    "        zf.extractall()\n",
    "        return inner_zip\n",
    "inner_zip=unzip(\"Python Assessment.zip\")\n",
    "for file in inner_zip:\n",
    "    unzip(file)"
